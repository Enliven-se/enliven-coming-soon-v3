- var link_classes = ['mbr-buttons-link', 'text-black']
- var navbar_items = data.navbar_items
section#ext_menu-0.mbr-navbar.mbr-navbar-freeze.mbr-navbar-absolute.mbr-navbar-sticky.mbr-navbar-auto-collapse.top
  nav.mbr-navbar-section.mbr-section.navbar.navbar-default
    .mbr-section-container
      .mbr-navbar-container.navbar-header.container
        .mbr-navbar-column.mbr-navbar-column-s.mbr-navbar-brand
          span.mbr-navbar-brand-link.mbr-brand.mbr-brand-inline
            span.mbr-brand-logo
              a(href='/')
                img.mbr-navbar-brand-img.mbr-brand-img(src='/images/enliven-logo.png', alt='Enliven')

        .mbr-navbar-hamburger.mbr-hamburger
          span.mbr-hamburger-line

        .mbr-navbar-column.mbr-navbar-menu
          nav.mbr-navbar-menu-box.mbr-navbar-menu-box-inline-right
            .mbr-navbar-column
              ul.mbr-navbar-items.mbr-navbar-items-right.float-left.mbr-buttons.mbr-buttons-freeze.mbr-buttons-right.btn-decorator.mbr-buttons-active.mbr-buttons-only-links.nav.navbar-nav.navbar-right
                each item in navbar_items
                  li.mbr-navbar-item
                    a(class=link_classes class=item.extra_classes class={active: item.className === page}, href=item.url)
                      | !{item.title}
